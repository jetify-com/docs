{
  "name": "Jetify Docs",
  "description": "Repo containing documentation for Jetify products.",
  "env": {
    "DEVBOX_COREPACK_ENABLED": "false"
  },
  "packages": {
    "nodejs": "24",
    "vale": "latest"
  },
  "shell": {
    "scripts": {
      "dev": "pnpm run dev $@",
      "fmt": "pnpm run fmt $@",
      "lint": "pnpm run lint $@",
      "mint": "pnpm run mint $@",
      "vale": "vale $@"
    }
  },
  "$schema": "https://raw.githubusercontent.com/jetify-com/devbox/main/.schema/devbox.schema.json"
}
